{
  "name": "next-py-reporters",
  "packageManager": "npm@9.6.2",
  "type": "module",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "test": "turbo run test --concurrency=1",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "wdio": "wdio run ./apps\\test-mocha\\wdio.conf.js"
  },
  "devDependencies": {
    "@wdio/local-runner": "^8.16.15",
    "@wdio/mocha-framework": "^8.16.15",
    "@wdio/spec-reporter": "^8.16.12",
    "prettier": "^3.0.3",
    "tsconfig": "*",
    "tsup": "^7.2.0",
    "turbo": "1.10.14"
  }
}
