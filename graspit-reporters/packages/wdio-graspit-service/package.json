		".": [
			{
				"types": "./dist/index.d.ts",
				"import": "./dist/index.js",
				"require": "./dist/index.cjs"
			},
			"./dist/index.cjs"
		],
		"./package.json": "./package.json"
	},
	"type": "module",
	"license": "MIT",
	"scripts": {
		"lint": "eslint src/*.ts",
		"build": "rimraf -g ./dist && tsup src/index.ts --format cjs,esm --dts"
	},
	"dependencies": {
		"@wdio/reporter": "^8.16.17",
		"log4js": "^6.9.1",
		"node-fetch": "^3.3.2"
	},
	"devDependencies": {
		"@types/node": "^20.8.0",
		"@wdio/types": "^8.16.12"
	}
