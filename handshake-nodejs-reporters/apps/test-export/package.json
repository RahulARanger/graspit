		"@wdio/spec-reporter": "^8.32.4",
		"@wdio/static-server-service": "^8.32.4",
		"handshake-utils": "0.4.3",
		"sqlite": "^5.1.1",
		"sqlite3": "^5.1.7"
	},
	"dependencies": {
		"@wdio/cli": "^8.32.4",
		"@wdio/mocha-framework": "8.32.4",
