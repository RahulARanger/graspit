{
	"name": "test-export",
	"version": "0.4.0",
	"type": "module",
	"private": true,
	"description": "tests for verifying the dashboard output",
	"main": "index.js",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"wdio": "wdio run ./wdio.conf.ts",
		"export": "cd ../../ && npx handshake export ../TestResults --out ../TestReports",
		"verify-sanity": "npx wdio run ./wdio.conf.ts --cucumberOpts.tagExpression=\"@sanity\"",
		"dev-sanity": "npx cross-env PORT=3000 wdio run ./wdio.conf.ts --cucumberOpts.tagExpression=\"@sanity\""
	},
	"keywords": [
		"webdriverio",
		"handshake",
		"tests"
	],
