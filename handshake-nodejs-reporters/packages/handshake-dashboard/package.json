{
    "name": "handshake-dashboard",
    "version": "0.5.1",
    "license": "MIT",
    "scripts": {
        "dev": "next dev",
        "build": "npm run lint && npm run build-antd",
        "start": "next start",
        "lint": "next lint",
        "predev": "ts-node --project ./tsconfig.node.json ./scripts/generate-antd-css.ts",
        "build-antd": "cross-env NODE_ENV=production ts-node --project ./tsconfig.node.json ./scripts/generate-antd-css.ts",
        "export": "cross-env NODE_ENV=production next build --no-lint",
        "test": "next lint",
        "e2e": "cd ../../ && npx handshake export ../TestResults --out ../TestReports",
        "preview": "cd ../../ && npx handshake display ../TestReports",
        "host-images": "cd ../../../TestResults && npx serve -p 4000",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build"
    },
    "dependencies": {
        "@ant-design/icons": "^5.3.5",
        "@mantine/carousel": "^7.6.2",
        "@mantine/charts": "^7.6.2",
        "@mantine/core": "^7.6.2",
        "@mantine/hooks": "^7.6.2",
        "@tabler/icons-react": "^3.1.0",
        "@tsconfig/node18": "^18.2.2",
        "@types/echarts": "^4.9.22",
        "@types/node": "20.11.30",
        "@types/react": "^18.2.69",
        "@types/react-dom": "18.2.22",
        "@uiw/react-markdown-preview": "^5.1.1",
        "ansi-to-html": "^0.7.2",
        "antd": "^5.15.3",
        "dayjs": "^1.11.10",
        "echarts": "^5.5.0",
        "echarts-for-react": "^3.0.2",
        "embla-carousel-react": "^8.0.0",
        "handshake-utils": "0.5.0",
        "next": "14.1.4",
        "react": "18.2.0",
        "react-countup": "^6.5.3",
        "react-dom": "18.2.0",
        "react-highlight-words": "^0.20.0",
        "recharts": "^2.12.3",
        "sqlite": "^5.1.1",
        "sqlite3": "^5.1.7",
        "swr": "^2.2.5",
        "tslib": "^2.6.2",
        "typescript": "^5.4.3"
    },
    "devDependencies": {
        "@ant-design/static-style-extract": "~1.0.2",
        "@chromatic-com/storybook": "^1.2.25",
        "@next/bundle-analyzer": "^14.1.4",
        "@next/eslint-plugin-next": "^14.1.4",
        "@storybook/addon-essentials": "^8.0.4",
        "@storybook/addon-interactions": "^8.0.4",
        "@storybook/addon-links": "^8.0.4",
        "@storybook/addon-mdx-gfm": "^8.0.4",
        "@storybook/addon-onboarding": "^8.0.4",
        "@storybook/blocks": "^8.0.4",
        "@storybook/nextjs": "^8.0.4",
        "@storybook/react": "^8.0.4",
        "@storybook/test": "^8.0.4",
        "@types/react-highlight-words": "^0.16.7",
        "@typescript-eslint/eslint-plugin": "^7.3.1",
        "@typescript-eslint/parser": "^7.3.1",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-jsx-a11y": "^6.8.0",
        "eslint-plugin-react": "^7.34.1",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-storybook": "^0.8.0",
        "eslint-plugin-unicorn": "^51.0.1",
