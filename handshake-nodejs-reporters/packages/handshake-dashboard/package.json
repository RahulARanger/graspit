{
    "name": "handshake-dashboard",
    "version": "0.5.1",
    "license": "MIT",
    "scripts": {
        "dev": "next dev",
        "build": "npm run lint && npm run build-antd",
        "start": "next start",
        "lint": "next lint",
        "build-antd": "cross-env NODE_ENV=production ts-node --project ./tsconfig.node.json ./scripts/generate-antd-css.ts",
        "export": "cross-env NODE_ENV=production next build",
        "test": "next lint",
        "e2e": "cd ../../ && npx handshake export ../TestResults --out ../TestReports",
        "preview": "cd ../../ && npx handshake display ../TestReports",
        "host-images": "cd ../../../TestResults && npx serve -p 4000",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build"
    },
