{
    "name": "handshake-dashboard",
    "version": "0.4.6",
    "license": "MIT",
    "scripts": {
        "dev": "next dev",
        "build": "npm run lint && npm run build-antd",
        "start": "next start",
        "lint": "next lint",
        "predev": "ts-node --project ./tsconfig.node.json ./scripts/generate-antd-css.ts",
        "build-antd": "cross-env NODE_ENV=production ts-node --project ./tsconfig.node.json ./scripts/generate-antd-css.ts",
        "export": "cross-env NODE_ENV=production next build --no-lint",
        "test": "next lint",
        "e2e": "cd ../../ && npx handshake export ../TestResults --out ../TestReports",
        "preview": "cd ../../ && npx handshake display ../TestReports"
    },
    "dependencies": {
        "@ant-design/icons": "^5.3.0",
        "@next/bundle-analyzer": "^14.1.0",
        "@tsconfig/node18": "^18.2.2",
        "@types/echarts": "^4.9.22",
        "@types/node": "20.11.16",
        "@types/react": "^18.2.55",
        "@types/react-dom": "18.2.18",
        "ansi-to-html": "^0.7.2",
        "antd": "^5.14.0",
        "dayjs": "^1.11.10",
        "echarts": "^5.4.3",
        "echarts-for-react": "^3.0.2",
        "embla-carousel-autoplay": "^8.0.0-rc22",
        "embla-carousel-react": "^8.0.0-rc22",
        "handshake-utils": "0.4.2",
        "next": "14.1.0",
        "react": "18.2.0",
        "react-countup": "^6.5.0",
        "react-dom": "18.2.0",
        "sqlite": "^5.1.1",
        "sqlite3": "^5.1.7",
        "swr": "^2.2.4",
        "tslib": "^2.6.2",
        "typescript": "^5.3.3"
    },
    "devDependencies": {
        "@ant-design/static-style-extract": "~1.0.2",
        "@next/eslint-plugin-next": "^14.1.0",
        "@typescript-eslint/eslint-plugin": "^6.21.0",
        "@typescript-eslint/parser": "^6.21.0",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-jsx-a11y": "^6.8.0",
        "eslint-plugin-react": "^7.33.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-unicorn": "^51.0.0",
        "prettier": "^3.2.5",
        "ts-node": "^10.9.2"
    }
}
