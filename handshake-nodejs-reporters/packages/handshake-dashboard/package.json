{
    "name": "handshake-dashboard",
    "version": "0.5.1",
    "license": "MIT",
    "scripts": {
        "dev": "next dev",
        "build": "npm run lint && npm run build-antd",
        "start": "next start",
        "lint": "next lint",
        "predev": "ts-node --project ./tsconfig.node.json ./scripts/generate-antd-css.ts",
        "build-antd": "cross-env NODE_ENV=production ts-node --project ./tsconfig.node.json ./scripts/generate-antd-css.ts",
        "export": "cross-env NODE_ENV=production next build --no-lint",
        "test": "next lint",
        "e2e": "cd ../../ && npx handshake export ../TestResults --out ../TestReports",
        "preview": "cd ../../ && npx handshake display ../TestReports",
        "host-images": "cd ../../../TestResults && npx serve -p 4000"
    },
    "dependencies": {
        "@ant-design/icons": "^5.3.1",
        "@tsconfig/node18": "^18.2.2",
        "@types/echarts": "^4.9.22",
        "@types/node": "20.11.27",
        "@types/react": "^18.2.66",
        "@types/react-dom": "18.2.22",
        "@uiw/react-markdown-preview": "^5.0.10",
        "ansi-to-html": "^0.7.2",
        "antd": "^5.15.2",
        "dayjs": "^1.11.10",
        "echarts": "^5.5.0",
        "echarts-for-react": "^3.0.2",
        "handshake-utils": "0.5.0",
        "next": "14.1.3",
        "react": "18.2.0",
        "react-countup": "^6.5.0",
        "react-dom": "18.2.0",
        "react-highlight-words": "^0.20.0",
        "sqlite": "^5.1.1",
        "sqlite3": "^5.1.7",
        "swr": "^2.2.5",
        "tslib": "^2.6.2",
        "typescript": "^5.4.2"
    },
    "devDependencies": {
        "@next/bundle-analyzer": "^14.1.3",
        "@ant-design/static-style-extract": "~1.0.2",
        "@next/eslint-plugin-next": "^14.1.3",
        "@types/react-highlight-words": "^0.16.7",
        "@typescript-eslint/eslint-plugin": "^7.2.0",
        "@typescript-eslint/parser": "^7.2.0",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-jsx-a11y": "^6.8.0",
        "eslint-plugin-react": "^7.34.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-unicorn": "^51.0.1",
        "prettier": "^3.2.5",
        "ts-node": "^10.9.2"
    }
}
