		"@types/async-lock": "^1.4.2",
		"@types/jest": "^29.5.12",
		"@types/mocha": "^10.0.6",
		"@types/node": "^20.11.27",
		"@types/superagent": "^8.1.4",
		"@types/supertest": "^6.0.2",
		"babel-jest": "^29.7.0",
		"babel-plugin-transform-import-meta": "^2.2.1",
